<?xml version="1.0" encoding="utf-16"?>
<ArrayOfOptionMetaRepositoryItem xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <OptionMetaRepositoryItem>
    <Key>Block Policy</Key>
    <Value>
      <Key>Block Policy</Key>
      <Code>block-policy</Code>
      <Text>Block Policy</Text>
	  <Group>Common</Group>
      <Tooltip>Sets the default behavior for filter rules that specify the block action.</Tooltip>
      <Default>drop</Default>
      <CanUseMultiple>false</CanUseMultiple>
      <Options>
        <anyType xsi:type="xsd:string">drop</anyType>
        <anyType xsi:type="xsd:string">return</anyType>
      </Options>
      <ContainsOptions>true</ContainsOptions>
      <FreeInput>false</FreeInput>
    </Value>
  </OptionMetaRepositoryItem>
  <OptionMetaRepositoryItem>
    <Key>State Policy</Key>
    <Value>
      <Key>State Policy</Key>
      <Code>state-policy</Code>
      <Text>State Policy</Text>
	  <Group>Common</Group>
      <Tooltip>Sets PF's behavior when it comes to keeping state. This behavior can be overridden on a per rule basis.</Tooltip>
      <Default>floating</Default>
      <CanUseMultiple>false</CanUseMultiple>
      <Options>
        <anyType xsi:type="xsd:string">if-bound</anyType>
        <anyType xsi:type="xsd:string">floating</anyType>
      </Options>
      <ContainsOptions>true</ContainsOptions>
      <FreeInput>false</FreeInput>
    </Value>
  </OptionMetaRepositoryItem>
  <OptionMetaRepositoryItem>
    <Key>Debug Level</Key>
    <Value>
      <Key>Debug Level</Key>
      <Code>debug</Code>
      <Text>Debug Level</Text>
	  <Group>Common</Group>
      <Tooltip>Set pf's debugging level.</Tooltip>
      <Default>urgent</Default>
      <CanUseMultiple>false</CanUseMultiple>
      <Options>
        <anyType xsi:type="xsd:string">none</anyType>
        <anyType xsi:type="xsd:string">urgent</anyType>
        <anyType xsi:type="xsd:string">misc</anyType>
        <anyType xsi:type="xsd:string">loud</anyType>
      </Options>
      <ContainsOptions>true</ContainsOptions>
      <FreeInput>false</FreeInput>
    </Value>
  </OptionMetaRepositoryItem>
  <OptionMetaRepositoryItem>
    <Key>Fingerprints</Key>
    <Value>
      <Key>Fingerprints</Key>
      <Code>fingerprints</Code>
      <Text>Fingerprints File</Text>
	  <Group>Common</Group>
      <Tooltip>Sets the file to load operating system fingerprints from. For use with passive OS fingerprinting.</Tooltip>
      <Default>&quot;/etc/pf.os&quot;</Default>
      <CanUseMultiple>false</CanUseMultiple>
      <ContainsOptions>false</ContainsOptions>
      <FreeInput>true</FreeInput>
      <Rules>
        <ValidationRule>NotEmptyText</ValidationRule>
      </Rules>
    </Value>
  </OptionMetaRepositoryItem>
  <OptionMetaRepositoryItem>
    <Key>Frags</Key>
    <Value>
      <Key>Frags</Key>
      <Code>limit frags</Code>
      <Text>Frags Limit</Text>
	  <Group>Limits</Group>
      <Tooltip>maximum number of entries in the memory pool used for packet reassembly (scrub rules).</Tooltip>
      <Default>5000</Default>
      <CanUseMultiple>false</CanUseMultiple>
      <ContainsOptions>false</ContainsOptions>
      <FreeInput>true</FreeInput>
      <Rules>
        <ValidationRule>PositiveInt</ValidationRule>
      </Rules>
    </Value>
  </OptionMetaRepositoryItem>
  <OptionMetaRepositoryItem>
    <Key>Src-Nodes</Key>
    <Value>
      <Key>Src-Nodes</Key>
      <Code>limit src-nodes</Code>
      <Text>Source Nodes</Text>
	  <Group>Limits</Group>
      <Tooltip>maximum number of entries in the memory pool used for tracking source IP addresses (generated by the sticky-address and source-track options).</Tooltip>
      <Default>10000</Default>
      <CanUseMultiple>false</CanUseMultiple>
      <ContainsOptions>false</ContainsOptions>
      <FreeInput>true</FreeInput>
      <Rules>
        <ValidationRule>PositiveInt</ValidationRule>
      </Rules>
    </Value>
  </OptionMetaRepositoryItem>
  <OptionMetaRepositoryItem>
    <Key>States</Key>
    <Value>
      <Key>States</Key>
      <Code>limit states</Code>
      <Text>States</Text>
	  <Group>Limits</Group>
      <Tooltip>maximum number of entries in the memory pool used for state table entries (filter rules that specify keep state).</Tooltip>
      <Default>10000</Default>
      <CanUseMultiple>false</CanUseMultiple>
      <ContainsOptions>false</ContainsOptions>
      <FreeInput>true</FreeInput>
      <Rules>
        <ValidationRule>PositiveInt</ValidationRule>
      </Rules>
    </Value>
  </OptionMetaRepositoryItem>
  <OptionMetaRepositoryItem>
    <Key>Tables</Key>
    <Value>
      <Key>Tables</Key>
      <Code>limit tables</Code>
      <Text>Tables</Text>
	  <Group>Limits</Group>
      <Tooltip>maximum number of tables that can be created.</Tooltip>
      <Default>1000</Default>
      <CanUseMultiple>false</CanUseMultiple>
      <ContainsOptions>false</ContainsOptions>
      <FreeInput>true</FreeInput>
      <Rules>
        <ValidationRule>PositiveInt</ValidationRule>
      </Rules>
    </Value>
  </OptionMetaRepositoryItem>
  <OptionMetaRepositoryItem>
    <Key>Table-Entries</Key>
    <Value>
      <Key>Table-Entries</Key>
      <Code>limit table-entries</Code>
      <Text>Table-Entries</Text>
	  <Group>Limits</Group>
      <Tooltip>the overall limit on how many addresses can be stored in all tables.</Tooltip>
      <Default>100000</Default>
      <CanUseMultiple>false</CanUseMultiple>
      <ContainsOptions>false</ContainsOptions>
      <FreeInput>true</FreeInput>
      <Rules>
        <ValidationRule>PositiveInt</ValidationRule>
      </Rules>
    </Value>
  </OptionMetaRepositoryItem>
  <OptionMetaRepositoryItem>
    <Key>Optimization</Key>
    <Value>
      <Key>Optimization</Key>
      <Code>optimization</Code>
      <Text>Optimization Mode</Text>
	  <Group>Optimization</Group>
      <Tooltip>Optimize PF for one of the network environments.</Tooltip>
      <Default>normal</Default>
      <CanUseMultiple>false</CanUseMultiple>
      <Options>
        <anyType xsi:type="xsd:string">normal</anyType>
        <anyType xsi:type="xsd:string">high-latency</anyType>
        <anyType xsi:type="xsd:string">aggressive</anyType>
        <anyType xsi:type="xsd:string">conservative</anyType>
      </Options>
      <ContainsOptions>true</ContainsOptions>
      <FreeInput>false</FreeInput>
    </Value>
  </OptionMetaRepositoryItem>
  <OptionMetaRepositoryItem>
    <Key>Ruleset-Optimization</Key>
    <Value>
      <Key>Ruleset-Optimization</Key>
      <Code>ruleset-optimization</Code>
      <Text>Ruleset Optimization</Text>
	  <Group>Optimization</Group>
      <Tooltip>Control operation of the PF ruleset optimizer.</Tooltip>
      <Default>basic</Default>
      <CanUseMultiple>false</CanUseMultiple>
      <Options>
        <anyType xsi:type="xsd:string">none</anyType>
        <anyType xsi:type="xsd:string">basic</anyType>
        <anyType xsi:type="xsd:string">profile</anyType>
      </Options>
      <ContainsOptions>true</ContainsOptions>
      <FreeInput>false</FreeInput>
    </Value>
  </OptionMetaRepositoryItem>
  <OptionMetaRepositoryItem>
    <Key>Loginterface</Key>
    <Value>
      <Key>Loginterface</Key>
      <Code>loginterface</Code>
      <Text>Loginterface</Text>
	  <Group>Interfaces</Group>
      <Tooltip>Sets the interface for which PF should gather statistics such as bytes in, out and packets passed/blocked. Statistics can only be gathered for one interface at a time. Note that the match, bad-offset, etc., counters and the state table counters are recorded regardless of whether loginterface is set or not. To turn this option off, set it to none.</Tooltip>
      <Default>none</Default>
      <CanUseMultiple>false</CanUseMultiple>
      <Options>
        <anyType xsi:type="xsd:string">none</anyType>
      </Options>
      <ContainsOptions>true</ContainsOptions>
      <FreeInput>true</FreeInput>
    </Value>
  </OptionMetaRepositoryItem>
  <OptionMetaRepositoryItem>
    <Key>SkipOn</Key>
    <Value>
      <Key>SkipOn</Key>
      <Code>skip on</Code>
      <Text>Skip On</Text>
	  <Group>Interfaces</Group>
      <Tooltip>Skip all PF processing on interface. This can be useful on loopback interfaces where filtering, normalization, queueing, etc, are not required. This option can be used multiple times.</Tooltip>
      <Default>none</Default>
      <CanUseMultiple>false</CanUseMultiple>
      <Options>
        <anyType xsi:type="xsd:string">none</anyType>
      </Options>
      <ContainsOptions>true</ContainsOptions>
      <FreeInput>true</FreeInput>
    </Value>
  </OptionMetaRepositoryItem>
  <OptionMetaRepositoryItem>
    <Key>Interval</Key>
    <Value>
      <Key>Interval</Key>
      <Code>timeout interval</Code>
      <Text>Interval</Text>
	  <Group>Timeouts</Group>
      <Tooltip>seconds between purges of expired states and packet fragments.</Tooltip>
      <Default>10</Default>
      <CanUseMultiple>false</CanUseMultiple>
      <ContainsOptions>false</ContainsOptions>
      <FreeInput>true</FreeInput>
      <Rules>
        <ValidationRule>PositiveInt</ValidationRule>
      </Rules>
    </Value>
  </OptionMetaRepositoryItem>
  <OptionMetaRepositoryItem>
    <Key>Frag</Key>
    <Value>
      <Key>Frag</Key>
      <Code>timeout frag</Code>
      <Text>Frag</Text>
	  <Group>Timeouts</Group>
      <Tooltip>seconds before an unassembled fragment is expired.</Tooltip>
      <Default>30</Default>
      <CanUseMultiple>false</CanUseMultiple>
      <ContainsOptions>false</ContainsOptions>
      <FreeInput>true</FreeInput>
      <Rules>
        <ValidationRule>PositiveInt</ValidationRule>
      </Rules>
    </Value>
  </OptionMetaRepositoryItem>
  <OptionMetaRepositoryItem>
    <Key>SrcTrack</Key>
    <Value>
      <Key>SrcTrack</Key>
      <Code>timeout src.track</Code>
      <Text>Source Tracking</Text>
	  <Group>Timeouts</Group>
      <Tooltip>seconds to keep a source tracking entry in memory after the last state expires.</Tooltip>
      <Default>0</Default>
      <CanUseMultiple>false</CanUseMultiple>
      <ContainsOptions>false</ContainsOptions>
      <FreeInput>true</FreeInput>
      <Rules>
        <ValidationRule>PositiveInt</ValidationRule>
      </Rules>
    </Value>
  </OptionMetaRepositoryItem>
</ArrayOfOptionMetaRepositoryItem>