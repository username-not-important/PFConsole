<Window xmlns:igWPF="http://schemas.infragistics.com/xaml/wpf" x:Class="PFConsole.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:visualResources="clr-namespace:PFConsole.VisualResources"
        xmlns:system="clr-namespace:System;assembly=mscorlib"
        xmlns:project="clr-namespace:PFConsole.Project"
        xmlns:ig="http://schemas.infragistics.com/xaml"
        xmlns:diagram="clr-namespace:PFConsole.Util.Diagram"
        xmlns:views="clr-namespace:PFConsole.Views"
        xmlns:converters="clr-namespace:PFConsole.Converters"
        Name="_Window"
        WindowState="Maximized" ResizeMode="CanResizeWithGrip"
        Title="PF Firewall Console" Height="595.62" Width="878.832" Style="{DynamicResource ArtParentWindowStyle}" FlowDirection="LeftToRight" MinWidth="400" MinHeight="250" Loaded="_Window_Loaded" Closing="_Window_Closing" ContentRendered="_Window_ContentRendered">
    <Window.CommandBindings>
        <CommandBinding Command="Close" Executed="CloseCommand_OnExecuted"/>
        <CommandBinding Command="New" Executed="NewCommand_OnExecuted" />
        <CommandBinding Command="Save" Executed="SaveCommand_OnExecuted" CanExecute="SaveCommand_CanExecute"/>
        <CommandBinding Command="Open" Executed="OpenCommand_OnExecuted" />
    </Window.CommandBindings>
    <Window.Resources>
        <DataTemplate x:Key="UndoRedoToolItem">
            <TextBlock Text="{Binding ShortDescription}"
                                   Margin="-30,-5"
                                   Padding="30,5">
                <ig:Commanding.Command>
                        <ig:UndoManagerCommandSource EventName="MouseLeftButtonDown"
                                                     CommandType="UndoRedoHistoryItem"
                                                     ParameterBinding="{Binding }" />    
                    </ig:Commanding.Command>
            </TextBlock>
        </DataTemplate>
        
        <converters:MacrosListToStringConverter x:Key="MacrosListToStringConverter"/>

    </Window.Resources>
    <Grid igWPF:ApplicationMenu2010.IsConstrainingElement="True">
        <ScrollViewer VerticalScrollBarVisibility="Disabled">
            <DockPanel LastChildFill="True">
                <igWPF:XamRibbon AllowMinimize="False" Theme="Office2013" ApplicationMenuMode="Office2010" DockPanel.Dock="Top" x:Name="_Ribbon">
                    <igWPF:XamRibbon.TabItemAreaToolbar>
                        <igWPF:TabItemAreaToolbar>
                            <Border />
                        </igWPF:TabItemAreaToolbar>
                    </igWPF:XamRibbon.TabItemAreaToolbar>
                    <igWPF:XamRibbon.ApplicationMenu2010>
                        <igWPF:ApplicationMenu2010 Caption="Firewall" KeyTip="F">
                            <igWPF:ApplicationMenu2010.Items>
                                <igWPF:ApplicationMenu2010Item Header="Info" />
                                <igWPF:ApplicationMenu2010Item Header="New Configuration" Command="New" CommandTarget="{Binding ElementName=_Window}"/>
                                <igWPF:ApplicationMenu2010Item Header="Open" Command="Open" CommandTarget="{Binding ElementName=_Window}"/>
                                <igWPF:ApplicationMenu2010Item Header="Save" Command="Save" CommandTarget="{Binding ElementName=_Window}"/>
                                <igWPF:ApplicationMenu2010Item Header="Save As" />
                                <igWPF:ApplicationMenu2010Item Header="Print"/>
                                <igWPF:SeparatorTool />
                                <igWPF:ApplicationMenu2010Item Header="Close" Click="_Button_CloseProject" IsEnabled="{Binding Source={x:Static project:PFProject.Instance}, Path=ProjectLoaded}"/>
                            </igWPF:ApplicationMenu2010.Items>
                        </igWPF:ApplicationMenu2010>
                    </igWPF:XamRibbon.ApplicationMenu2010>
                    <igWPF:RibbonTabItem Header="Macros" KeyTip="M" IsEnabled="{Binding Source={x:Static project:PFProject.Instance}, Path=ProjectLoaded}">
                        <igWPF:RibbonGroup Caption="Assign" IsEnabled="{Binding Source={x:Static project:PFProject.Instance}, Path=ProjectLoaded}">
                            <igWPF:ButtonTool Caption="Assign Macro" Click="_Button_AssignMacro_Clicked" igWPF:RibbonGroup.MinimumSize="ImageAndTextLarge" igWPF:RibbonGroup.MaximumSize="ImageAndTextLarge" ToolTip="Assign A macro to an interface, ip address, etc...">
                                <igWPF:ButtonTool.LargeImage>
                                    <BitmapImage UriSource="/PFConsole;component/VisualResources/Images/Ribbon/Ribbon_Macros_Add.png"/>
                                </igWPF:ButtonTool.LargeImage>
                            </igWPF:ButtonTool>
                        </igWPF:RibbonGroup>
                    </igWPF:RibbonTabItem>
                    <igWPF:RibbonTabItem Header="Tables" KeyTip="T" IsEnabled="{Binding Source={x:Static project:PFProject.Instance}, Path=ProjectLoaded}" >
                        <igWPF:RibbonGroup Caption="Create" IsEnabled="{Binding Source={x:Static project:PFProject.Instance}, Path=ProjectLoaded}">
                            <igWPF:ButtonTool Caption="Create Table" Click="_Button_CreateTable_Clicked" igWPF:RibbonGroup.MinimumSize="ImageAndTextLarge" igWPF:RibbonGroup.MaximumSize="ImageAndTextLarge" ToolTip="Create a new table.">
                                <igWPF:ButtonTool.LargeImage>
                                    <BitmapImage UriSource="/PFConsole;component/VisualResources/Images/Ribbon/Ribbon_Tables_Create.png"/>
                                </igWPF:ButtonTool.LargeImage>
                            </igWPF:ButtonTool>
                        </igWPF:RibbonGroup>
                    </igWPF:RibbonTabItem>
                    <igWPF:RibbonTabItem Header="Options" KeyTip="O" IsEnabled="{Binding Source={x:Static project:PFProject.Instance}, Path=ProjectLoaded}">
                        <igWPF:RibbonGroup Caption="View" IsEnabled="{Binding Source={x:Static project:PFProject.Instance}, Path=ProjectLoaded}">
                            <igWPF:ButtonTool Caption="View Options" igWPF:RibbonGroup.MinimumSize="ImageAndTextLarge" igWPF:RibbonGroup.MaximumSize="ImageAndTextLarge" ToolTip="View, Edit or Delete Options." Click="_Button_ViewOptions_Clicked">
                                <igWPF:ButtonTool.LargeImage>
                                    <BitmapImage UriSource="/PFConsole;component/VisualResources/Images/Ribbon/Ribbon_Options_View.png"/>
                                </igWPF:ButtonTool.LargeImage>
                            </igWPF:ButtonTool>
                            <igWPF:MenuTool Caption="" ToolTip="Options Text Size" igWPF:RibbonGroup.MaximumSize="ImageOnly">
                                <igWPF:MenuTool.SmallImage>
                                    <BitmapImage UriSource="VisualResources/Images/Ribbon/Ribbon_Options_FontSize.png"/>
                                </igWPF:MenuTool.SmallImage>
                                <igWPF:MenuTool.Items>
                                    <igWPF:ButtonTool Content="Small" Tag="13" Click="Ribbon_Options_FontSize_ItemClicked" />
                                    <igWPF:ButtonTool Content="Medium" Tag="15" Click="Ribbon_Options_FontSize_ItemClicked" />
                                    <igWPF:ButtonTool Content="Large" Tag="17" Click="Ribbon_Options_FontSize_ItemClicked" />
                                </igWPF:MenuTool.Items>
                            </igWPF:MenuTool>
                            <igWPF:MenuTool Caption="" ToolTip="Options Display Mode" igWPF:RibbonGroup.MaximumSize="ImageOnly">
                                <igWPF:MenuTool.SmallImage>
                                    <BitmapImage UriSource="VisualResources/Images/Ribbon/Ribbon_Options_DisplayMode.png"/>
                                </igWPF:MenuTool.SmallImage>
                                <igWPF:MenuTool.Items>
                                    <igWPF:ButtonTool Content="List" Tag="List" Click="Ribbon_Options_DisplayMode_ItemClicked" />
                                    <igWPF:ButtonTool Content="Cards" Tag="Cards" Click="Ribbon_Options_DisplayMode_ItemClicked" />
                                </igWPF:MenuTool.Items>
                            </igWPF:MenuTool>
                            <igWPF:MenuTool Caption="" ToolTip="Cards Grouping Mode" igWPF:RibbonGroup.MaximumSize="ImageOnly">
                                <igWPF:MenuTool.SmallImage>
                                    <BitmapImage UriSource="VisualResources/Images/Ribbon/Ribbon_Options_GrouppingMode.png"/>
                                </igWPF:MenuTool.SmallImage>
                                <igWPF:MenuTool.Items>
                                    <igWPF:ButtonTool Content="Not Groupped" Tag="None" Click="Ribbon_Options_GrouppingMode_ItemClicked" />
                                    <igWPF:ButtonTool Content="Groupped by Option Kind" Tag="Group" Click="Ribbon_Options_GrouppingMode_ItemClicked" />
                                </igWPF:MenuTool.Items>
                            </igWPF:MenuTool>
                        </igWPF:RibbonGroup>
                        <igWPF:RibbonGroup Caption="Manage" IsEnabled="{Binding Source={x:Static project:PFProject.Instance}, Path=ProjectLoaded}">
                            <igWPF:MenuTool Caption="Modify Option" MenuItemDescriptionMinWidth="50" ToolTip="Add new option settings, or modify the existing ones.">
                                <igWPF:MenuTool.LargeImage>
                                    <BitmapImage UriSource="/PFConsole;component/VisualResources/Images/Ribbon/Ribbon_Options_Add.png"/>
                                </igWPF:MenuTool.LargeImage>
                                <igWPF:GalleryTool PreferredDropDownColumns="5" ItemClicked="_ModifyOption_ItemClicked">
                                    <igWPF:GalleryTool.Resources>
                                        <ResourceDictionary Source="VisualResources/Images/Ribbon/Options_Gallery/OptionsGalleryDictionary.xaml" />
                                    </igWPF:GalleryTool.Resources>
                                    <igWPF:GalleryTool.Groups>
                                        <igWPF:GalleryItemGroup Title="Common">
                                            <igWPF:GalleryItemGroup.ItemKeys>
                                                <system:String>Block Policy</system:String>
                                                <system:String>State Policy</system:String>
                                                <system:String>Debug Level</system:String>
                                                <system:String>Fingerprints</system:String>
                                            </igWPF:GalleryItemGroup.ItemKeys>
                                        </igWPF:GalleryItemGroup>
                                        <igWPF:GalleryItemGroup Title="Limits">
                                            <igWPF:GalleryItemGroup.ItemKeys>
                                                <system:String>Frags</system:String>
                                                <system:String>Src-Nodes</system:String>
                                                <system:String>States</system:String>
                                                <system:String>Tables</system:String>
                                                <system:String>Table-Entries</system:String>
                                            </igWPF:GalleryItemGroup.ItemKeys>
                                        </igWPF:GalleryItemGroup>
                                        <igWPF:GalleryItemGroup Title="Optimization">
                                            <igWPF:GalleryItemGroup.ItemKeys>
                                                <system:String>Optimization</system:String>
                                                <system:String>Ruleset-Optimization</system:String>
                                            </igWPF:GalleryItemGroup.ItemKeys>
                                        </igWPF:GalleryItemGroup>
                                        <igWPF:GalleryItemGroup Title="Interface">
                                            <igWPF:GalleryItemGroup.ItemKeys>
                                                <system:String>Loginterface</system:String>
                                                <system:String>SkipOn</system:String>
                                            </igWPF:GalleryItemGroup.ItemKeys>
                                        </igWPF:GalleryItemGroup>
                                        <igWPF:GalleryItemGroup Title="Timeouts">
                                            <igWPF:GalleryItemGroup.ItemKeys>
                                                <system:String>Interval</system:String>
                                                <system:String>Frag</system:String>
                                                <system:String>SrcTrack</system:String>
                                            </igWPF:GalleryItemGroup.ItemKeys>
                                        </igWPF:GalleryItemGroup>
                                    </igWPF:GalleryTool.Groups>
                                    <igWPF:GalleryTool.Items>
                                        <igWPF:GalleryItem Key="Block Policy" Text="Block Policy" Image="VisualResources/Images/Ribbon/Options_Gallery/Ribbon_Options_Gallery_BlockPolicy.png" />
                                        <igWPF:GalleryItem Key="State Policy" Text="State Policy" Image="VisualResources/Images/Ribbon/Options_Gallery/Ribbon_Options_Gallery_StatePolicy.png" />
                                        <igWPF:GalleryItem Key="Debug Level" Text="Debugging Level" Image="VisualResources/Images/Ribbon/Options_Gallery/Ribbon_Options_Gallery_DebugLevel.png" />
                                        <igWPF:GalleryItem Key="Fingerprints" Text="Fingerprints File" Image="VisualResources/Images/Ribbon/Options_Gallery/Ribbon_Options_Gallery_FingerPrints.png" />

                                        <igWPF:GalleryItem Key="Frags" Text="Frags Limit" Image="VisualResources/Images/Ribbon/Options_Gallery/Ribbon_Options_Gallery_Frags.png" />
                                        <igWPF:GalleryItem Key="Src-Nodes" Text="Source Nodes" Image="VisualResources/Images/Ribbon/Options_Gallery/Ribbon_Options_Gallery_Src-Nodes.png" />
                                        <igWPF:GalleryItem Key="States" Text="States" Image="VisualResources/Images/Ribbon/Options_Gallery/Ribbon_Options_Gallery_States.png" />
                                        <igWPF:GalleryItem Key="Tables" Text="Tables" Image="VisualResources/Images/Ribbon/Options_Gallery/Ribbon_Options_Gallery_Tables.png" />
                                        <igWPF:GalleryItem Key="Table-Entries" Text="Table Entries" Image="VisualResources/Images/Ribbon/Options_Gallery/Ribbon_Options_Gallery_Table-Entries.png" />

                                        <igWPF:GalleryItem Key="Optimization" Text="Optimization Mode" Image="VisualResources/Images/Ribbon/Options_Gallery/Ribbon_Options_Gallery_Optimization.png" />
                                        <igWPF:GalleryItem Key="Ruleset-Optimization" Text="Ruleset Optimization" Image="VisualResources/Images/Ribbon/Options_Gallery/Ribbon_Options_Gallery_Ruleset-Optimization.png" />

                                        <igWPF:GalleryItem Key="Loginterface" Text="Loginterface" Image="VisualResources/Images/Ribbon/Options_Gallery/Ribbon_Options_Gallery_Loginterface.png" />
                                        <igWPF:GalleryItem Key="SkipOn" Text="Skip On" Image="VisualResources/Images/Ribbon/Options_Gallery/Ribbon_Options_Gallery_SkipOn.png" />

                                        <igWPF:GalleryItem Key="Interval" Text="Interval" Image="VisualResources/Images/Ribbon/Options_Gallery/Ribbon_Options_Gallery_Interval.png" />
                                        <igWPF:GalleryItem Key="Frag" Text="Fragment Expiration" Image="VisualResources/Images/Ribbon/Options_Gallery/Ribbon_Options_Gallery_Frag.png" />
                                        <igWPF:GalleryItem Key="SrcTrack" Text="Source Tracking" Image="VisualResources/Images/Ribbon/Options_Gallery/Ribbon_Options_Gallery_SrcTrack.png" />

                                    </igWPF:GalleryTool.Items>
                                </igWPF:GalleryTool>
                            </igWPF:MenuTool>
                            <igWPF:MenuTool Caption="Option Sets" ToolTip="Save or Load presets of modified options.">
                                <igWPF:MenuTool.LargeImage>
                                    <BitmapImage UriSource="/PFConsole;component/VisualResources/Images/Ribbon/Ribbon_Options_OptionSets.png"/>
                                </igWPF:MenuTool.LargeImage>
                                <igWPF:LabelTool Content="Custom Presets" />
                                <igWPF:ButtonTool Content="All Default" />
                                <igWPF:LabelTool Content="Manage Presets" />
                                <igWPF:ButtonTool Content="Add Current State as Preset" />
                            </igWPF:MenuTool>
                        </igWPF:RibbonGroup>
                    </igWPF:RibbonTabItem>
                    <igWPF:RibbonTabItem Header="Filtering" KeyTip="F" IsEnabled="{Binding Source={x:Static project:PFProject.Instance}, Path=ProjectLoaded}">
                        <igWPF:RibbonGroup Caption="Manage">
                            <igWPF:ButtonTool Caption="New Filtering Diagram" igWPF:RibbonGroup.MinimumSize="ImageAndTextLarge" igWPF:RibbonGroup.MaximumSize="ImageAndTextLarge" Click="_Button_NewDiagram_Click">
                                <igWPF:ButtonTool.LargeImage>
                                    <BitmapImage UriSource="/PFConsole;component/VisualResources/Images/Ribbon/Ribbon_Filtering_NewDiagram.png"/>
                                </igWPF:ButtonTool.LargeImage>
                            </igWPF:ButtonTool>
                        </igWPF:RibbonGroup>
                    </igWPF:RibbonTabItem>
                    <igWPF:RibbonTabItem Header="Queing" KeyTip="Q" IsEnabled="{Binding Source={x:Static project:PFProject.Instance}, Path=ProjectLoaded}"/>

                    <igWPF:XamRibbon.ContextualTabGroups>
                        <igWPF:ContextualTabGroup Key="DiagramGroup" Caption="PF Diagram" IsVisible="{Binding Source={x:Static visualResources:DiagramManager.Instance}, Path=IsDiagramInView}" x:Name="_DiagramContextualGroup">
                            <igWPF:ContextualTabGroup.Tabs>
                                <igWPF:RibbonTabItem Header="Diagram Tools" KeyTip="D" IsEnabled="{Binding Source={x:Static project:PFProject.Instance}, Path=ProjectLoaded}"
                                                     DataContext="{Binding Source={x:Static visualResources:DiagramManager.Instance}, Path=ActiveDiagram}">
                                    <igWPF:RibbonGroup Caption="History">
                                        <igWPF:MenuTool Caption="Undo" igWPF:RibbonGroup.MaximumSize="ImageAndTextLarge" ButtonType="Segmented" 
                                                        Command="Undo" CommandTarget="{Binding}" ItemsSource="{Binding UndoManager.UndoHistory}"
                                                        ItemTemplate="{StaticResource UndoRedoToolItem}">
                                            <igWPF:MenuTool.LargeImage>
                                                <BitmapImage UriSource="/PFConsole;component/VisualResources/Images/Ribbon/Diagram_Tools/Ribbon_Diagram_Tools_Undo.png"/>
                                            </igWPF:MenuTool.LargeImage>
                                        </igWPF:MenuTool>
                                        <igWPF:MenuTool Caption="Redo" igWPF:RibbonGroup.MaximumSize="ImageAndTextLarge" ButtonType="Segmented"
                                                        Command="Redo" CommandTarget="{Binding}" ItemsSource="{Binding UndoManager.RedoHistory}"
                                                        ItemTemplate="{StaticResource UndoRedoToolItem}">
                                            <igWPF:MenuTool.LargeImage>
                                                <BitmapImage UriSource="/PFConsole;component/VisualResources/Images/Ribbon/Diagram_Tools/Ribbon_Diagram_Tools_Redo.png"/>
                                            </igWPF:MenuTool.LargeImage>
                                        </igWPF:MenuTool>
                                    </igWPF:RibbonGroup>
                                    <igWPF:RibbonGroup Caption="Selection">
                                        <igWPF:ButtonTool Caption="Paste" igWPF:RibbonGroup.MaximumSize="ImageAndTextLarge"
                                                          Command="Paste" CommandTarget="{Binding}">
                                            <igWPF:ButtonTool.LargeImage>
                                                <BitmapImage UriSource="/PFConsole;component/VisualResources/Images/Ribbon/Diagram_Tools/Ribbon_Diagram_Tools_Paste.png"/>
                                            </igWPF:ButtonTool.LargeImage>
                                        </igWPF:ButtonTool>
                                        <igWPF:ButtonTool Caption="Cut"
                                                          Command="Cut" CommandTarget="{Binding}">
                                            <igWPF:ButtonTool.LargeImage>
                                                <BitmapImage UriSource="/PFConsole;component/VisualResources/Images/Ribbon/Diagram_Tools/Ribbon_Diagram_Tools_Cut.png"/>
                                            </igWPF:ButtonTool.LargeImage>
                                        </igWPF:ButtonTool>
                                        <igWPF:ButtonTool Caption="Copy"
                                                          Command="Copy" CommandTarget="{Binding}">
                                            <igWPF:ButtonTool.LargeImage>
                                                <BitmapImage UriSource="/PFConsole;component/VisualResources/Images/Ribbon/Diagram_Tools/Ribbon_Diagram_Tools_Copy.png"/>
                                            </igWPF:ButtonTool.LargeImage>
                                        </igWPF:ButtonTool>
                                        <igWPF:ButtonTool Caption="Delete"
                                                          Command="Delete" CommandTarget="{Binding}">
                                            <igWPF:ButtonTool.LargeImage>
                                                <BitmapImage UriSource="/PFConsole;component/VisualResources/Images/Ribbon/Diagram_Tools/Ribbon_Diagram_Tools_Delete.png"/>
                                            </igWPF:ButtonTool.LargeImage>
                                        </igWPF:ButtonTool>
                                        <igWPF:ButtonTool Caption="Edit"
                                                          Command="{x:Static ig:DiagramCommands.EnterEditMode}" CommandTarget="{Binding}">
                                            <igWPF:ButtonTool.LargeImage>
                                                <BitmapImage UriSource="/PFConsole;component/VisualResources/Images/Ribbon/Diagram_Tools/Ribbon_Diagram_Tools_Edit.png"/>
                                            </igWPF:ButtonTool.LargeImage>
                                        </igWPF:ButtonTool>
                                    </igWPF:RibbonGroup>
                                    <igWPF:RibbonGroup Caption="ZIndex">
                                        <igWPF:ButtonTool Caption="Bring Forward"
                                                          Command="{x:Static ig:DiagramCommands.BringForward}" CommandTarget="{Binding}">
                                            <igWPF:ButtonTool.LargeImage>
                                                <BitmapImage UriSource="/PFConsole;component/VisualResources/Images/Ribbon/Diagram_Tools/Ribbon_Diagram_Tools_BringForward.png"/>
                                            </igWPF:ButtonTool.LargeImage>
                                        </igWPF:ButtonTool>

                                        <igWPF:ButtonTool Caption="Bring To Front"
                                                          Command="{x:Static ig:DiagramCommands.BringToFront}" CommandTarget="{Binding}">
                                            <igWPF:ButtonTool.LargeImage>
                                                <BitmapImage UriSource="/PFConsole;component/VisualResources/Images/Ribbon/Diagram_Tools/Ribbon_Diagram_Tools_BringToFront.png"/>
                                            </igWPF:ButtonTool.LargeImage>
                                        </igWPF:ButtonTool>
                                        <igWPF:ButtonTool Caption="Send Backward"
                                                          Command="{x:Static ig:DiagramCommands.SendBackward}" CommandTarget="{Binding}">
                                            <igWPF:ButtonTool.LargeImage>
                                                <BitmapImage UriSource="/PFConsole;component/VisualResources/Images/Ribbon/Diagram_Tools/Ribbon_Diagram_Tools_SendBackward.png"/>
                                            </igWPF:ButtonTool.LargeImage>
                                        </igWPF:ButtonTool>
                                        <igWPF:ButtonTool Caption="Send To Back"
                                                          Command="{x:Static ig:DiagramCommands.SendToBack}" CommandTarget="{Binding}">
                                            <igWPF:ButtonTool.LargeImage>
                                                <BitmapImage UriSource="/PFConsole;component/VisualResources/Images/Ribbon/Diagram_Tools/Ribbon_Diagram_Tools_SendToBack.png"/>
                                            </igWPF:ButtonTool.LargeImage>
                                        </igWPF:ButtonTool>
                                    </igWPF:RibbonGroup>
                                    <igWPF:RibbonGroup Caption="Tools">
                                        <igWPF:RadioButtonTool Caption="Select Tool" IsChecked="True" Checked="Ribbon_DiagramTools_ToolChanged" Tag="Select">
                                            <igWPF:RadioButtonTool.LargeImage>
                                                <BitmapImage UriSource="/PFConsole;component/VisualResources/Images/Ribbon/Diagram_Tools/Ribbon_Diagram_Tools_SelectTool.png"/>
                                            </igWPF:RadioButtonTool.LargeImage>
                                        </igWPF:RadioButtonTool>
                                        <igWPF:RadioButtonTool Caption="Connect Tool" Checked="Ribbon_DiagramTools_ToolChanged" Tag="Connect">
                                            <igWPF:RadioButtonTool.LargeImage>
                                                <BitmapImage UriSource="/PFConsole;component/VisualResources/Images/Ribbon/Diagram_Tools/Ribbon_Diagram_Tools_ConnectTool.png"/>
                                            </igWPF:RadioButtonTool.LargeImage>
                                        </igWPF:RadioButtonTool>
                                    </igWPF:RibbonGroup>
                                    <igWPF:RibbonGroup Caption="Drag Interaction">
                                        <igWPF:RadioButtonTool Caption="Select" IsChecked="True" Checked="Ribbon_DiagramTools_DragInteractionChanged">
                                            <igWPF:RadioButtonTool.LargeImage>
                                                <BitmapImage UriSource="/PFConsole;component/VisualResources/Images/Ribbon/Diagram_Tools/Ribbon_Diagram_Tools_DragSelect.png"/>
                                            </igWPF:RadioButtonTool.LargeImage>
                                        </igWPF:RadioButtonTool>
                                        <igWPF:RadioButtonTool Caption="Pan" Checked="Ribbon_DiagramTools_DragInteractionChanged">
                                            <igWPF:RadioButtonTool.LargeImage>
                                                <BitmapImage UriSource="/PFConsole;component/VisualResources/Images/Ribbon/Diagram_Tools/Ribbon_Diagram_Tools_DragPan.png"/>
                                            </igWPF:RadioButtonTool.LargeImage>
                                        </igWPF:RadioButtonTool>
                                        <igWPF:RadioButtonTool Caption="Zoom" Checked="Ribbon_DiagramTools_DragInteractionChanged">
                                            <igWPF:RadioButtonTool.LargeImage>
                                                <BitmapImage UriSource="/PFConsole;component/VisualResources/Images/Ribbon/Diagram_Tools/Ribbon_Diagram_Tools_DragZoom.png"/>
                                            </igWPF:RadioButtonTool.LargeImage>
                                        </igWPF:RadioButtonTool>
                                        <igWPF:RadioButtonTool Caption="None" Checked="Ribbon_DiagramTools_DragInteractionChanged">
                                            <igWPF:RadioButtonTool.LargeImage>
                                                <BitmapImage UriSource="/PFConsole;component/VisualResources/Images/Ribbon/Diagram_Tools/Ribbon_Diagram_Tools_DragNone.png"/>
                                            </igWPF:RadioButtonTool.LargeImage>
                                        </igWPF:RadioButtonTool>
                                    </igWPF:RibbonGroup>
                                </igWPF:RibbonTabItem>
                            </igWPF:ContextualTabGroup.Tabs>

                        </igWPF:ContextualTabGroup>
                    </igWPF:XamRibbon.ContextualTabGroups>
                </igWPF:XamRibbon>
                <igWPF:XamDockManager Theme="Office2013" Name="_DockManager">
                    <igWPF:XamDockManager.Panes>
                        <igWPF:SplitPane>
                            <igWPF:ContentPane Header="Firewall Explorer" CloseButtonVisibility="Collapsed" AllowClose="False" MinWidth="250" IsPinned="False" x:Name="_Pane_FirewallExplorer">
                                <DockPanel LastChildFill="True">
                                    <StackPanel Orientation="Horizontal" DockPanel.Dock="Top" />
                                    <TreeView FontWeight="Thin">
                                        <TreeView.Items>
                                            <TreeViewItem Header="{Binding Source={x:Static project:PFProject.Instance}, Path=Title}" IsExpanded="True" Style="{DynamicResource ProjectItemStyle}" IsEnabled="{Binding ProjectLoaded, Source={x:Static project:PFProject.Instance}}">
                                                <TreeViewItem.ContextMenu>
                                                    <ContextMenu MinWidth="200">
                                                        <MenuItem Header="Build Configuration File" />
                                                        <MenuItem Header="Add">
                                                            <MenuItem Header="Interface" Click="_MenuItem_AddInterface_Clicked"/>
                                                            <MenuItem Header="Macro" />
                                                            <MenuItem Header="Table" />
                                                            <MenuItem Header="Diagram" />
                                                        </MenuItem>
                                                    </ContextMenu>
                                                </TreeViewItem.ContextMenu>
                                                <TreeViewItem Header="Interfaces" IsExpanded="True" x:Name="TreeItem_Interfaces" HeaderTemplate="{StaticResource DefaultNodeDataTemplate}" ItemTemplate="{StaticResource InterfaceNodeTemplate}" ItemsSource="{Binding Source={x:Static project:PFProject.Instance}, Path=Interfaces.Interfaces}" />
                                                <TreeViewItem Header="Options" MouseDoubleClick="_FirewallExplorer_Options_DoubleClick" HeaderTemplate="{StaticResource DefaultNodeDataTemplate}"/>
                                                <TreeViewItem Header="Macros" HeaderTemplate="{StaticResource DefaultNodeDataTemplate}" ItemsSource="{Binding Source={x:Static project:PFProject.Instance}, Path=Macros.Macros}" ItemTemplate="{StaticResource MacroNodeTemplate}"/>
                                                <TreeViewItem Header="Tables" HeaderTemplate="{StaticResource DefaultNodeDataTemplate}" ItemsSource="{Binding Source={x:Static project:PFProject.Instance}, Path=Tables.Tables}" ItemTemplate="{StaticResource TableNodeTemplate}"/>
                                                <TreeViewItem Header="Diagrams" HeaderTemplate="{StaticResource DefaultNodeDataTemplate}" ItemsSource="{Binding Source={x:Static project:PFProject.Instance}, Path=Diagrams.Diagrams}" ItemTemplate="{StaticResource DiagramNodeTemplate}"/>
                                              
                                            </TreeViewItem>
                                        </TreeView.Items>
                                        <TreeView.ItemContainerStyle>
                                            <Style TargetType="{x:Type TreeViewItem}">
                                                <!-- Style for the selected item -->
                                                <Setter Property="BorderThickness" Value="1"/>
                                                <Style.Triggers>
                                                    <!-- Selected and has focus -->
                                                    <Trigger Property="IsSelected" Value="True">
                                                        <Setter Property="BorderBrush" Value="#7DA2CE"/>
                                                    </Trigger>
                                                    <!-- Mouse over -->
                                                    <Trigger Property="IsMouseOver" Value="True">
