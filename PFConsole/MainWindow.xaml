                        <igWPF:SplitPane igWPF:XamDockManager.InitialLocation="DockedRight" Width="250" >
                            <igWPF:ContentPane Header="Properties" CloseButtonVisibility="Collapsed" AllowClose="False" AllowDockingFloating="False" MinWidth="100" x:Name="_Pane_Properties">
                                <ig:XamPropertyGrid MinWidth="100" Name="_Prop">
                                    <ig:XamPropertyGrid.EditorDefinitions>
                                        <ig:PropertyGridEditorDefinition>
                                            <ig:PropertyGridEditorDefinition.TargetProperties>
                                                <system:String>MaxSrcConnRate</system:String>
                                            </ig:PropertyGridEditorDefinition.TargetProperties>
                                            <ig:PropertyGridEditorDefinition.EditTemplate>
                                                <DataTemplate>
                                                    <Grid>
                                                        <ig:XamMaskedInput BorderBrush="Transparent" PromptChar=" " InvalidValueBehavior="RevertValue" Value="{Binding Path=Value}" BorderThickness="0" SectionTabNavigation="NextSection">
                                                            <ig:XamMaskedInput.Mask>
                                                                <system:String>{number}/{number}</system:String>
                                                            </ig:XamMaskedInput.Mask>
                                                        </ig:XamMaskedInput>
                                                    </Grid>
                                                </DataTemplate>
                                            </ig:PropertyGridEditorDefinition.EditTemplate>
                                        </ig:PropertyGridEditorDefinition>
                                        <ig:PropertyGridEditorDefinition>
                                            <ig:PropertyGridEditorDefinition.TargetProperties>
                                                <system:String>TCPFlags</system:String>
                                            </ig:PropertyGridEditorDefinition.TargetProperties>
                                            <ig:PropertyGridEditorDefinition.EditTemplate>
                                                <DataTemplate>
                                                    <Grid>
                                                        <views:FlagCheckMaskView />
                                                    </Grid>
                                                </DataTemplate>
                                            </ig:PropertyGridEditorDefinition.EditTemplate>
                                        </ig:PropertyGridEditorDefinition>
                                        <ig:PropertyGridEditorDefinition>
                                            <ig:PropertyGridEditorDefinition.TargetProperties>
                                                <system:String>SourceAddress</system:String>
                                                <system:String>DestinationAddress</system:String>
                                            </ig:PropertyGridEditorDefinition.TargetProperties>
                                            <ig:PropertyGridEditorDefinition.EditTemplate>
                                                <DataTemplate>
                                                    <Grid>
                                                        <TextBox Margin="0,0,25,0" Text="{Binding Value, Converter={StaticResource MacrosListToStringConverter}}" IsReadOnly="True" />
                                                        <Button Style="{StaticResource FlatButton}" Content="..." Width="25" HorizontalAlignment="Right" Click="PropertyGrid_IPAddressList_Edit" Tag="{Binding Value}" />
                                                    </Grid>
                                                </DataTemplate>
                                            </ig:PropertyGridEditorDefinition.EditTemplate>
                                        </ig:PropertyGridEditorDefinition>
                                        <ig:PropertyGridEditorDefinition>
                                            <ig:PropertyGridEditorDefinition.TargetProperties>
                                                <system:String>SourcePort</system:String>
                                                <system:String>DestinationPort</system:String>
                                            </ig:PropertyGridEditorDefinition.TargetProperties>
                                            <ig:PropertyGridEditorDefinition.EditTemplate>
                                                <DataTemplate>
                                                    <Grid>
                                                        <TextBox Margin="0,0,25,0" Text="{Binding Value, Converter={StaticResource MacrosListToStringConverter}}" IsReadOnly="True" />
                                                        <Button Style="{StaticResource FlatButton}" Content="..." Width="25" HorizontalAlignment="Right" Click="PropertyGrid_PortList_Edit" Tag="{Binding Value}" />
                                                    </Grid>
                                                </DataTemplate>
                                            </ig:PropertyGridEditorDefinition.EditTemplate>
                                        </ig:PropertyGridEditorDefinition>
                                    </ig:XamPropertyGrid.EditorDefinitions>
                                    <ig:XamPropertyGrid.PropertyGenerator>
                                        <diagram:DiagramItemPropertyGenerator />
                                    </ig:XamPropertyGrid.PropertyGenerator>
                                </ig:XamPropertyGrid>
                            </igWPF:ContentPane>
                        </igWPF:SplitPane>
                        <igWPF:SplitPane SplitterOrientation="Horizontal" igWPF:XamDockManager.InitialLocation="DockedBottom">
                            <igWPF:TabGroupPane>
                                <igWPF:ContentPane x:Name="_Pane_Output" Header="Output" CloseButtonVisibility="Collapsed" MinHeight="150" AllowClose="False">
                                    <DockPanel LastChildFill="True" Margin="3">
                                        <Grid DockPanel.Dock="Top">
                                            <Button x:Name="_Button_OutputClear" Content="Clear" HorizontalAlignment="Left" Style="{DynamicResource FlatButton}" Click="_Button_OutputClear_Click" />
                                        </Grid>
                                        <RichTextBox IsReadOnly="True" Margin="0,3,0,0" Name="_OutputBox" Block.LineHeight="10" VerticalScrollBarVisibility="Auto">
                                            <RichTextBox.Document>
                                                <FlowDocument />
                                            </RichTextBox.Document>
                                        </RichTextBox>
                                    </DockPanel>
                                </igWPF:ContentPane>
                                <igWPF:ContentPane x:Name="_Pane_Preview" Header="Preview" CloseButtonVisibility="Collapsed" MinHeight="150" AllowClose="False">
                                    <DockPanel LastChildFill="True">
                                        <Button Content="Generate Preview" HorizontalAlignment="Left" Click="_Button_GenPreview_Click" DockPanel.Dock="Top"></Button>
                                        <RichTextBox Name="_PreviewBox" IsReadOnly="True" FontFamily="Courier New" FontSize="16" VerticalScrollBarVisibility="Visible" HorizontalScrollBarVisibility="Auto">

                                        </RichTextBox>
                                    </DockPanel>
                                </igWPF:ContentPane>
                            </igWPF:TabGroupPane>
                        </igWPF:SplitPane>
                    </igWPF:XamDockManager.Panes>
                    <igWPF:DocumentContentHost Background="{Binding Source={x:Static visualResources:ThemeManager.Instance}, Path=ThemeBackgroundColor}">
                        <igWPF:SplitPane>
                            <igWPF:TabGroupPane Name="_MainContentPane" >
                                <igWPF:ContentPane x:Name="_Pane_Dashboard" Header="Dashboard" AllowClose="False" AllowDocking="False" CloseButtonVisibility="Collapsed" AllowFloatingOnly="False">
                                    <Frame Source="ContentPages/DashboardPage.xaml" NavigationUIVisibility="Hidden" />
                                </igWPF:ContentPane>
                            </igWPF:TabGroupPane>
                        </igWPF:SplitPane>
                    </igWPF:DocumentContentHost>
                </igWPF:XamDockManager>
            </DockPanel>
        </ScrollViewer>
    </Grid>
</Window>
